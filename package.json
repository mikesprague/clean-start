{
  "name": "clean-start",
  "version": "0.30.0",
  "description": "Open source new tab extension",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "build": "vite build",
    "build:extension": "cross-env BUILD_TYPE=extension NODE_ENV=production vite build",
    "clean": "rm -rf ./build",
    "dev": "vite serve",
    "prebuild": "npm run clean && cp ./src/pwa.html ./src/index.html",
    "prebuild:extension": "npm run clean && cp ./src/extension.html ./src/index.html",
    "postbuild": "rm ./src/index.html",
    "postbuild:extension": "rm ./build/_headers ./src/index.html && npm run zip",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=development wrangler pages dev -- vite serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --color",
    "zip": "cd build && zip -r clean-start-extension.zip ./"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/clean-start"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://cleanstart.page",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/clean-start/issues"
  },
  "dependencies": {
    "@tippyjs/react": "4.2.6",
    "axios": "0.27.2",
    "cheerio": "1.0.0-rc.12",
    "dayjs": "1.11.5",
    "dompurify": "2.4.0",
    "dotenv": "16.0.2",
    "he": "1.2.0",
    "nanoid": "4.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "register-service-worker": "1.7.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.9",
    "@babel/eslint-plugin": "7.18.10",
    "@babel/preset-env": "7.19.0",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@tailwindcss/typography": "0.5.7",
    "@tippyjs/react": "4.2.6",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.8",
    "concurrently": "7.3.0",
    "cross-env": "7.0.3",
    "cssnano": "5.1.13",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "16.0.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.6",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.5.0",
    "postcss": "8.4.16",
    "postcss-cli": "10.0.0",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "sass": "1.54.8",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "28.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "tailwindcss": "3.1.8",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "workbox-webpack-plugin": "6.5.4",
    "wrangler": "2.0.28",
    "vite": "3.1.0",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-pwa": "0.12.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "jest": {
        "version": 28
      }
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "allowImportExportEverywhere": true,
      "parser": "@eslint/babel-parser",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jest/no-deprecated-functions": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "consistent-return": "off"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
