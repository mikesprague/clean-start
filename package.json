{
  "name": "clean-start",
  "version": "0.36.17",
  "description": "Open source new tab extension",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "build:extension": "cross-env BUILD_TYPE=extension vite build",
    "build": "vite build",
    "clean": "rm -rf ./build",
    "dev": "vite serve",
    "postbuild:extension": "rm ./build/_headers ./build/_redirects ./src/index.html ./build/*.map && npm run zip",
    "postbuild": "rm ./src/index.html",
    "poststart": "npm run postbuild",
    "prebuild:extension": "npm run clean && cp ./src/extension.html ./src/index.html",
    "prebuild": "npm run clean && cp ./src/pwa.html ./src/index.html",
    "prestart": "npm run prebuild",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=development wrangler --compatibility-date=2022-10-31 pages dev -- vite serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "zip": "cd build && zip -r clean-start-extension.zip ./"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/clean-start"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://cleanstart.page",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/clean-start/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@tippyjs/react": "4.2.6",
    "axios": "1.3.5",
    "cheerio": "1.0.0-rc.12",
    "dayjs": "1.11.7",
    "dompurify": "3.0.1",
    "he": "1.2.0",
    "nanoid": "4.0.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tippy.js": "6.3.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@babel/eslint-plugin": "7.19.1",
    "@babel/preset-env": "7.21.4",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@tailwindcss/typography": "0.5.9",
    "@vitejs/plugin-react-swc": "3.2.0",
    "autoprefixer": "10.4.14",
    "cross-env": "7.0.3",
    "cssnano": "6.0.0",
    "cz-git": "^1.6.1",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-stylelint": "18.0.0",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.7.1",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.7",
    "prettier-eslint": "15.0.1",
    "sass": "1.60.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-prettier": "3.0.0",
    "tailwindcss": "3.3.1",
    "vite": "4.2.1",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-pwa": "0.14.7",
    "wrangler": "2.14.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "jest": {
        "version": 28
      }
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:markdown/recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "allowImportExportEverywhere": true,
      "parser": "@eslint/babel-parser",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jest/no-deprecated-functions": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "import/prefer-default-export": "off",
      "no-console": "warn",
      "node/no-unpublished-import": "warn",
      "node/no-missing-import": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
