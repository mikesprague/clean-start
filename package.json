{
  "name": "clean-start",
  "version": "0.8.1",
  "description": "Open source new tab extension",
  "private": true,
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 6.x",
    "yarn": ">=1.x"
  },
  "scripts": {
    "build": "yarn webpack && yarn lambda:build",
    "build:extension": "yarn clean && cross-env BUILD_TYPE=extension yarn webpack && yarn zip",
    "build:local": "yarn clean && yarn webpack && yarn lambda:build",
    "clean": "rm -rf ./build && rm -rf ./lambda",
    "lambda:build": "netlify-lambda build ./src/functions",
    "lambda:serve": "netlify-lambda serve ./src/functions --config ./functions.config.js",
    "prepublish": "yarn snyk-protect",
    "snyk-protect": "snyk protect",
    "start": "concurrently 'yarn webpack:serve' 'yarn lambda:serve' --names 'ðŸ“¦,ðŸ’»' --prefix '[{name}] ' --kill-others",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "cross-env NODE_ENV=production webpack --color",
    "webpack:dev": "cross-env NODE_ENV=development webpack --color",
    "webpack:serve": "cross-env NODE_ENV=development webpack serve",
    "zip": "cd build && zip -r clean-start-extension.zip ./"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/clean-start"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://cleanst.art",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/clean-start/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@hot-loader/react-dom": "17.0.1",
    "@tailwindcss/typography": "0.3.1",
    "@tippyjs/react": "4.2.0",
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.5",
    "dayjs": "1.10.1",
    "dompurify": "2.2.6",
    "he": "1.2.0",
    "nanoid": "3.1.20",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0",
    "register-service-worker": "1.7.2",
    "snyk": "1.437.3",
    "tailwindcss": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-transform-object-assign": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/runtime": "7.12.5",
    "@fullhuman/postcss-purgecss": "3.1.3",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "autoprefixer": "10.1.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "compression-webpack-plugin": "7.1.1",
    "concurrently": "5.3.0",
    "copy-webpack-plugin": "7.0.0",
    "cross-env": "7.0.3",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "1.1.5",
    "cssnano": "4.1.10",
    "dotenv": "8.2.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-html": "6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.0.0-alpha.17",
    "inquirer": "7.3.3",
    "mini-css-extract-plugin": "1.3.3",
    "netlify-cli": "2.69.11",
    "netlify-lambda": "2.0.2",
    "node-sass": "5.0.0",
    "postcss": "8.2.1",
    "postcss-cli": "8.3.1",
    "postcss-loader": "4.1.0",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "prettier-stylelint": "0.4.2",
    "sass-lint": "1.13.1",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.0.3",
    "typescript": "4.1.3",
    "webpack": "5.11.0",
    "webpack-cli": "4.3.0",
    "webpack-dev-server": "3.11.0",
    "workbox-webpack-plugin": "6.0.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-export-default-from",
      "react-hot-loader/babel"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks",
      "security"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "react/jsx-boolean-value": "warn",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "no-param-reassign": "warn",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "prettier": {
    "plugins": [
      "all"
    ],
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabwidth": true,
    "trailingComma": "all"
  }
}
